{
  "name": "ecoinfra",
  "version": "1.0.0",
  "description": "",
  "target": "es6",
  "module": "commonjs",
  "main": "./build/main.js",
  "bin": {
    "ecoinfra": "./dist/main.js"
  },
  "scripts": {
    "build": "tsc",
    "package": "pkg .",
    "test": "find tests -name '*.test.ts' | xargs node --import tsx --test",
    "run": "ts-node app/main.ts",
    "lint": "eslint . --ext .ts,.test.ts",
    "lint:fix": "eslint . --ext .ts,.test.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eco-infra/ecoinfra.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eco-infra/ecoinfra/issues"
  },
  "homepage": "https://eco-infra.com",
  "pkg": {
    "scripts": "dist/**/*.js",
    "targets": [
      "node18-linux",
      "node18-windows",
      "node18-macos"
    ],
    "assets": [
      "node_modules/figlet/fonts/Standard.flf"
    ],
    "outputPath": "build"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@types/asciichart": "^1.5.8",
    "@types/cli-table": "^0.3.4",
    "@types/figlet": "^1.5.8",
    "@types/listr": "^0.14.4",
    "@types/node": "^20.11.20",
    "@types/node-fetch": "^2.6.2",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "pkg": "^5.8.1",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint-parser": "^22.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.231.0",
    "@types/chalk": "^2.2.0",
    "arg": "^5.0.2",
    "asciichart": "^1.5.25",
    "chalk": "^5.2.0",
    "cli-table": "^0.3.11",
    "enquirer": "^2.3.6",
    "figlet": "^1.6.0",
    "hcl2-parser": "^1.0.3",
    "kleur": "^4.1.5",
    "node-fetch": "^2.6.7"
  }
}
